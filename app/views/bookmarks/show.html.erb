<% provide(:title, 'Show Bookmarks') %>
<p id="notice"><%= notice %></p>
<h1>Showing Entered Bookmark</h1>
<p>
  <strong>Url:</strong>
  <%= @bookmark.url %>
</p>

<p>
  <strong>Name:</strong>
  <%= @bookmark.name %>
</p>

<p>
  <strong>Saved at:</strong>
  <%= @bookmark.saved_at %>
</p>

<%= link_to 'Edit', edit_bookmark_path(@bookmark) %> |
<%= link_to 'Back', bookmarks_path %>
